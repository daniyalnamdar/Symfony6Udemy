
{% extends  'base.html.twig' %}
{%  block body %}
<div>
    {% if  messages|length > 0 %}
    {% for key, message in messages|slice(0, limit) %}
        <div>
           <a href="{{ path('show_one', {id: key}) }}">{{ message.message }}</a>
        </div>
        <div>
      {{ include('hello/_message.html.twig', {message: message})}}
        </div>
    {% endfor %}
    {% else %}
        There is nothing here yet!
    {% endif %}
</div>
{% endblock %}